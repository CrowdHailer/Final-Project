Update
<%= @user.name %>
<%= @user.github_username %>
<%= @user.email %>
<%= image_tag 'test.jpg' %>
<%= 'You are not yet confirmed as a maker' unless @user.verified_maker %>

<%= 'Updates saved' if @saved %>

<%= form_for(@user, url: {action: "update_details"}) do |f| %>
	<%= f.text_field :email %>
	<%= f.text_field :cohort %>
	<%= f.text_field :profile_image %>
	<%= f.submit "Save" %>
<% end %>